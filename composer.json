{
    "name": "tiderjian/la-filepond",
    "description": "file upload extension for laravel-admin",
    "type": "laravel-admin,fileupload,filepond",
    "require": {
        "php": ">=7.2.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "laravel/laravel": "~5.7",
        "phpunit/phpunit": "^7.4",
        "laravel/dusk": "~5.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Qs\\La\\Filepond\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qs\\La\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "tiderjian",
            "email": "164710115@qq.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Qs\\La\\Filepond\\FilepondServiceProvider"
            ]
        }
    }
}
